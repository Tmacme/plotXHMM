#!/usr/bin/env python3.6

from plotxhmm.plotxhmm import Parser, Interval

print('Hello, World!')

parser = Parser('tests/normalized_zscores.txt')

print(parser._df.shape)
print(parser._df.index)

interval_a = Interval('22', 1000, 2000)
interval_b = Interval('22', 2001, 2500)

print(interval_a.overlaps(interval_b))
